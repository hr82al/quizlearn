generator client {
  provider = "prisma-client-js"
}
 
datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}
 
enum Quiz_Type {
  FILL
}

model Quiz {
  id        Int      @id @default(autoincrement())
  question  String   @unique
  quiz_type Quiz_Type
  quiz      String   
  answer    String
}